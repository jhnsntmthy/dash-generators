<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
  <head>
    <title><%%= h(yield(:title) || "Untitled") %></title>

    <%%= stylesheet_link_tag 'reset', 'dashboard', 'navigation_tabs', 'navigation_icons', 'forms_tables' %>
		<!--[if IE]><%%= stylesheet_link_tag 'ie' %><![endif]-->
  	<link rel="shortcut icon" href="/favicon.ico" />

	  <%%= javascript_include_tag 'jquery-1.2.6.min' %>
	  <%%= javascript_include_tag "jquery-ui", "jquery-fx", "jquery-pages", "thickbox" %>
		<%%= javascript_include_tag 'tablesorter', 'jquery.tablesorter.pager','tablesorter_filter', 'jquery.maskedinput','jquery.example', 'application_pager' %>

		<%#= javascript_include_tag :defaults %>

    <%%= yield(:head) %>

  </head>
  <body>

	<%%= render :partial => "layouts/header" %>

	<%%= render :partial => "layouts/tabs" %>
	

	
	<div id="page">
	  <div class="container clearfix">
	    <div id="left">
	      <div id="sub-nav" class="clearfix">
	        <ul>
				<%%= yield :subnav %>
	        </ul>
	      </div>
	      <div id="left_content">
		
			      
	      <%%- if show_title? -%>
	        <h1><%%=h yield(:title) %></h1>
	      <%%- end -%>
      
	      <%%= yield %>
	
	      </div>
	    </div>
	    <div id="right">
	      <div id="right_content">


	  	  <%%= yield :right_column %>




	      </div>
	    </div>
	  </div>
	</div>

	<div id="footer">
		<%%= render :partial => "layouts/footer" %>
	</div>

	<%%= render :partial => 'shared/google_analytics' %>
  </body>
</html>
